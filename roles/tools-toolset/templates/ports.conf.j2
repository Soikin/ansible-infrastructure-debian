Listen {{ apache_default_http_port }}

{% if apache_default_https_port is defined %}
<IfModule ssl_module>
   Listen {{ apache_default_https_port }}
</IfModule>

<IfModule mod_gnutls.c>
   Listen {{ apache_default_https_port }}
</IfModule>
