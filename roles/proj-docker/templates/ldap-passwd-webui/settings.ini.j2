[html]
page_title = Change your domain account password

[ldap:0]
host = {{ docker_rit.ldap_passwd_webui.host }}
port = {{ docker_rit.ldap_passwd_webui.port }}
use_ssl = {{ docker_rit.ldap_passwd_webui.use_ssl }}
base = {{ docker_rit.ldap_passwd_webui.base }}
type = ad
ad_domain = {{ docker_rit.ldap_passwd_webui.ad_domain }}
search_filter = sAMAccountName={uid}

[server]
server = auto
host = 0.0.0.0
port = 8080
